<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Servidor" namespace="Servidor.Model" default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
  <class name="ContaCaixaDTO" table="conta_caixa" >
    <id name="Id" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="Empresa" column="ID_EMPRESA" />
    <many-to-one name="AgenciaBanco" column="ID_AGENCIA_BANCO" />
    <property name="Codigo" column="CODIGO" />
    <property name="Digito" column="DIGITO" />
    <property name="Nome" column="NOME" />
    <property name="Descricao" column="DESCRICAO" />
    <property name="Tipo" column="TIPO" />
    <property name="LimiteCredito" column="LIMITE_CREDITO" />
    <property name="ClassificacaoContabilConta" column="CLASSIFICACAO_CONTABIL_CONTA" />
  </class>
</hibernate-mapping>