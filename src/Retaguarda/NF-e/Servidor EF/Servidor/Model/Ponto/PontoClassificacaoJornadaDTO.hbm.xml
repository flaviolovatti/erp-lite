<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Servidor" namespace="Servidor.Model" default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
  <class name="PontoClassificacaoJornadaDTO" table="ponto_classificacao_jornada" >
    <id name="Id" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="Empresa" column="ID_EMPRESA" />
    <property name="Codigo" column="CODIGO" />
    <property name="Nome" column="NOME" />
    <property name="Descricao" column="DESCRICAO" />
    <property name="Padrao" column="PADRAO" />
    <property name="DescontarHoras" column="DESCONTAR_HORAS" />
  </class>
</hibernate-mapping>