<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="VendasService" namespace="VendasService.Model" 
                   default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
  <class name="CondicoesParcelaDTO" table="venda_condicoes_parcelas" >
